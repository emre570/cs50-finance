#include <stdio.h>
#include <stdlib.h>
#include <cs50.h>

int main()
{
    long numara = get_long("Kart Numarasını Giriniz:");
    int basamak;
    basamak = 1;
    do
    {
        numara = numara/10;
        basamak++;
    }
    while(numara>=10);
    if (basamak<13 || basamak>16)
    {
    printf("GEÇERSİZ\n");
    }
}
